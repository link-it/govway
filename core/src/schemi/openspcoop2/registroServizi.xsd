				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="byte-specifica-conversazione-concettuale" type="xsd:base64Binary" use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<linkit:generator
						xmlns:linkit="http://www.link.it/project_generator">
						<linkit:ws-field-mapping filter="false"/>
