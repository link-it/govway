		<!-- start-extensions -->
		<xsd:attribute name="super-user" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<linkit:generator
						xmlns:linkit="http://www.link.it/project_generator">
						<linkit:ws-field-mapping transient="true"/>
					</linkit:generator>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<!-- end-extensions -->
	</xsd:complexType>
	

	
	
	
	
	



	<!-- Gruppi -->
	<xsd:element name="gruppo" type="gruppo" />
