	<!-- Scope -->
	
	<xsd:element name="scope" type="scope" />
	<xsd:complexType name="scope">
		<xsd:annotation>
			<xsd:appinfo>
				<linkit:generator xmlns:linkit="http://www.link.it/project_generator">
					<linkit:interface crud="true" id="id-scope" >
						<linkit:ws-interface crud="true" all="true" >
							<linkit:ws-filter sortCondition="false"/>
						</linkit:ws-interface>
					</linkit:interface>
				</linkit:generator>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:sequence>
			<!-- Proprieta Oggetto -->
			<xsd:element name="proprieta-oggetto" type="proprieta-oggetto" maxOccurs="1" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="nome" type="xsd:string" use="required"/>
		<xsd:attribute name="descrizione" type="xsd:string" use="optional"/>
		<xsd:attribute name="tipologia" type="xsd:string" use="optional"/> <!-- caratteristica dello scope (es. policy di gestione) -->
		<xsd:attribute name="nome-esterno" type="xsd:string" use="optional"/>
		<xsd:attribute name="contesto-utilizzo" type="ScopeContesto" use="optional" default="qualsiasi"/>
		<xsd:attribute name="ora-registrazione" type="xsd:dateTime">
			<xsd:annotation>
				<xsd:appinfo>
					<linkit:generator
						xmlns:linkit="http://www.link.it/project_generator">
						<linkit:ws-field-mapping filter="false" filter-interval="true"/>
					</linkit:generator>
				</xsd:appinfo>
			</xsd:annotation>
