								<linkit:sql-ddl-column-comment>Lunghezza del payload del Messaggio</linkit:sql-ddl-column-comment>
								<linkit:ws-field-mapping filter="false" />
							</linkit:generator>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
