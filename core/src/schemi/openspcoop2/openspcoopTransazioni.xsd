	<xsd:element name="dump-messaggio">
		<xsd:annotation>
			<xsd:appinfo>
				<linkit:generator xmlns:linkit="http://www.link.it/project_generator">
					<linkit:interface crud="true" id="id-dump-messaggio">
						<linkit:ws-interface search="true" crud="false" all="false" >
							<linkit:ws-search-method-interface inUse="false" />
							<linkit:ws-filter orCondition="false"/>
						</linkit:ws-interface>
					</linkit:interface>
				    <linkit:sql-table-mapping name="dump_messaggi" sequence-cycle="true"
				    	comment="Dump dei messaggi transitati nella Porta di Dominio">
						<!-- Indici per ottimizzare le ricerche basate sull'id della trasazione -->
						<linkit:sql-table-index>
							<linkit:sql-table-index-field name="id-transazione"/>
						</linkit:sql-table-index>
					    <!-- Indici per il post process -->
					    <linkit:sql-table-index>
					        <linkit:sql-table-index-field name="post-processed"/>
					    	<linkit:sql-table-index-field name="post-process-timestamp" ignoreTypeCompatibility="true"/>
					    </linkit:sql-table-index>
					    <linkit:sql-table-index>
					        <linkit:sql-table-index-column name="post_process_config_id"/>
					    </linkit:sql-table-index>
					</linkit:sql-table-mapping>
				</linkit:generator>
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id-transazione" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<linkit:generator
								xmlns:linkit="http://www.link.it/project_generator">
								<linkit:sql-ddl-column-comment>Identificativo della transazione a cui appartiene il messaggio</linkit:sql-ddl-column-comment>
