						<xsd:appinfo>
							<linkit:generator
								xmlns:linkit="http://www.link.it/project_generator">
								<linkit:sql-ddl-column-comment>Data che permette di raggruppare tutti i messaggi inviati o ricevuti durante una consegna</linkit:sql-ddl-column-comment>
								<linkit:ws-field-mapping filter="false" />
							</linkit:generator>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="tipo-messaggio" type="tipo-messaggio">
