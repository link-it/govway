<jboss-deployment-structure>
	<deployment>
		<!-- Exclusions allow you to prevent the server from automatically adding some dependencies     -->
		<exclude-subsystems>
			<subsystem name="webservices" />
			<subsystem name="jaxrs" />
			<subsystem name="jsf" />
			<subsystem name="logging"/>
			<!-- se si elimina, spuntano i warning <subsystem name="weld" /> -->
		</exclude-subsystems>
		<exclusions>
			<!-- avendo giÃ  escluso subsystem 'logging', questi moduli non sono caricati 
			<module name="org.slf4j" />
			<module name="org.apache.log4j" />
			<module name="org.apache.commons.logging"/>
			<module name="org.apache.logging.log4j.api"/>
			-->
			<!-- non caricato in wf18 e java 11
			<module name="org.apache.cxf" />
			<module name="org.springframework.spring" />
			<module name="org.jboss.as.webservices" />
			<module name="org.jboss.ws" />
			<module name="org.jboss.ws.*" />
			-->
		</exclusions>
	</deployment>
</jboss-deployment-structure>
