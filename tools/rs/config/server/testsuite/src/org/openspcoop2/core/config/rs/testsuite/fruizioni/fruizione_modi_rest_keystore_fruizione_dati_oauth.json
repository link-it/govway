{
    "connettore": {
        "endpoint": "https://ginovadifretta.it/petstore",
	"token_policy": "api-config-test-jwt"
    },
    "autenticazione": {
        "tipo": "https",
        "opzionale": false
    },
   

    "api_nome": "PetStore",
    "api_versione": 2,
	"modi": {
		"protocollo": "rest",
		"richiesta": {
			"sicurezza_messaggio": {
				"riferimento_x509": ["x5c"]
			},
			"oauth":{
				"identificativo": "clientId",
				"kid": "KIDvalue"
			}
		},
		"risposta": {
			"sicurezza_messaggio": {
				"truststore": {
					"modalita": "default"
				}
			}
		}
	}
    
}
