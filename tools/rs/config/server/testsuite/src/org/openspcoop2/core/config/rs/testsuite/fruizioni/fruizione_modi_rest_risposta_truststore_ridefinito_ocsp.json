{
    "connettore": {
        "endpoint": "https://ginovadifretta.it/petstore"
    },
    "autenticazione": {
        "tipo": "https",
        "opzionale": false
    },
   

    "api_nome": "PetStore",
    "api_versione": 2,
	"modi": {
		"protocollo": "rest",
		"richiesta": {
			"sicurezza_messaggio": {
				"riferimento_x509": ["x5c"]
			}
		},
		"risposta": {
			"sicurezza_messaggio": {
				"truststore": {
					"modalita": "ridefinito",
					"truststore_path": "/path/to/truststore",
					"truststore_password": "123456",
					"truststore_ocsp_policy" : "ocspPolicyID",
					"truststore_tipo": "jks"
				}
			}
		}
	}
    
}
