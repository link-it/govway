{
	"connettore": {
		"endpoint": "https://ginovadifretta.it/petstore"
	},
	"autenticazione": {
		"tipo": "https",
		"opzionale": false
	},
	"api_nome": "PetStore",
	"api_soap_servizio": "EsempioServizioComposto",
	"api_versione": 1,
	"modi": {
		"protocollo": "soap",
		"richiesta": {
			"sicurezza_messaggio": {
				"truststore": {
					"modalita": "default"
				}
			}
		},
		"risposta": {
			"sicurezza_messaggio": {
				"keystore": {
					"modalita": "ridefinito",
					"dati_keystore": {
						"tipologia": "hsm",
						"pcks11_tipo" : "pkcs11-modi-client",
		        			"key_alias" : "modi_client2_hsm"
					}
				}
			}
		}
	}
}
