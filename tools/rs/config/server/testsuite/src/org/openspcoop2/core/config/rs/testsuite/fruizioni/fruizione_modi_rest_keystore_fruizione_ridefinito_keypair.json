{
    "connettore": {
        "endpoint": "https://ginovadifretta.it/petstore"
    },
    "autenticazione": {
        "tipo": "https",
        "opzionale": false
    },
   

    "api_nome": "PetStore",
    "api_versione": 2,
	"modi": {
		"protocollo": "rest",
		"richiesta": {
			"sicurezza_messaggio": {
				"riferimento_x509": ["x5c"],
				"keystore": {
					"modalita": "ridefinito",
					"dati_keystore": {
						"tipologia": "filesystem",
						"keystore_path": "/path/to/chiaveprivata",
						"public_key_path": "/path/to/chiavepubblica",
						"key_password": "123456",
						"keystore_tipo": "keys"
					}
				}
			}
		},
		"risposta": {
			"sicurezza_messaggio": {
				"truststore": {
					"modalita": "default"
				}
			}
		}
	}
    
}
