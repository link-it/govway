	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/commons/pages/welcome.xhtml?usaSVG=true</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/public/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editUser</from-outcome>
			<to-view-id>/core/pages/form/user.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>about</from-outcome>
			<to-view-id>/commons/pages/about.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>statusPdD</from-outcome>
			<to-view-id>/commons/pages/statoPdd.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logoutAS</from-outcome>
			<to-view-id>/public/logoutAS.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>modalita</from-outcome>
			<to-view-id>/commons/pages/modalita.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>soggettoPddMonitor</from-outcome>
			<to-view-id>/commons/pages/soggettoPddMonitor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/public/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginSuccess</from-outcome>
			<to-view-id>/commons/pages/welcome.jsf?usaSVG=true</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>utentePasswordChange</from-outcome>
			<to-view-id>/core/pages/form/userPasswordScaduta.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/core/pages/form/userPasswordScaduta.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginSuccess</from-outcome>
			<to-view-id>/commons/pages/welcome.jsf?usaSVG=true</to-view-id>
		</navigation-case>
	</navigation-rule>
