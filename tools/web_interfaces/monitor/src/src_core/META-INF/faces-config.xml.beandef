	<managed-bean>
		<managed-bean-name>statusBean</managed-bean-name>
		<managed-bean-class>org.openspcoop2.web.monitor.core.mbean.StatusBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>utentiBean</managed-bean-name>
		<managed-bean-class>org.openspcoop2.web.monitor.core.mbean.UtentiBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.openspcoop2.web.monitor.core.dao.IUserService</property-class>
			<value>#{userService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>utenteBean</managed-bean-name>
		<managed-bean-class>org.openspcoop2.web.monitor.core.mbean.UtentiBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
			<property-class>org.openspcoop2.web.monitor.core.dao.IUserService</property-class>
			<value>#{userService}</value>
		</managed-property>
		<managed-property>
			<property-name>userToUpdate</property-name>
			<property-class>java.lang.String</property-class>
			<value>#{loginBean.username}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>applicationBean</managed-bean-name>
