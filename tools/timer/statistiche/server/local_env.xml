<?xml version="1.0" encoding="iso-8859-1"?>

<project name="GovWayStatistiche_LocalEnv" basedir=".">

      <!-- root di OpenSPCoop -->
      <property name="openspcoop2" location="../../../.."/>
      <property name="openspcoop2.dist" location="${openspcoop2}/core/dist"/>
      <property name="openspcoop2_jars" location="${openspcoop2}/dist"/>

      <!-- ApplicationServer version -->
      <property name="as" value="wildfly26" />

      <property name="required_lib_statistiche" location="${openspcoop2}/lib/"/>
      <property name="required_lib" value="${required_lib_statistiche}"/>
      <property name="openspcoop2.lib" location="${required_lib_statistiche}"/>

      <!-- Build-Options -->
      <property name="buildAsRelease" value="false"/>
      <property name="buildStatistiche" value="true"/>
      <property name="buildAPI" value="false"/>

      <!-- Specifica se inserire le informazioni di debug nelle classi -->
      <property name="debug" value="true" />
      <property name="printServerInfo" value="false" />

</project>
