	<target name="build_stub_cxf_from_url" depends="init_compile_ws,-build_stub_cxf_from_url"/>
	
	<target name="build_stub_cxf_from_wsdl" depends="init_compile_ws,-build_stub_cxf_from_wsdl"/>

	
	
