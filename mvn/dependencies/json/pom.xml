<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.openspcoop2</groupId>
	<version>${revision}</version>
	<artifactId>org.openspcoop2.json</artifactId>
	<packaging>pom</packaging>
	<name>dependencies.json</name>

	<parent>
		<groupId>org.openspcoop2</groupId>
		<artifactId>org.openspcoop2.dependencies</artifactId>
		<version>${revision}</version>
		<relativePath>../pom.xml</relativePath>
	</parent>

	<properties>

		<jar.group.name>json</jar.group.name>
		
		<org.json.json.version>20231013</org.json.json.version>

		<networknt_json-schema-validator.version>1.0.86</networknt_json-schema-validator.version>
		<com-ethlo-time.version>1.7.3</com-ethlo-time.version>

		<github-java-json-tools_json-schema-validator.version>2.2.14</github-java-json-tools_json-schema-validator.version>
		<github-java-json-tools_json-schema-validator.gov4j.version>2.2.14-gov4j-1</github-java-json-tools_json-schema-validator.gov4j.version>
		<github-java-json-tools_json-schema-core.version>1.2.14</github-java-json-tools_json-schema-core.version>
		<github-java-json-tools_jackson-coreutils.version>2.0</github-java-json-tools_jackson-coreutils.version>
		<github-java-json-tools_jackson-coreutils-equivalence.version>1.0</github-java-json-tools_jackson-coreutils-equivalence.version>
		<github-java-json-tools_uri-template.version>0.10</github-java-json-tools_uri-template.version>
		<btf.version>1.2</btf.version>
		<msg-simple.version>1.1</msg-simple.version>
		<failureaccess.version>1.0.1</failureaccess.version>
		<libphonenumber.version>8.13.18</libphonenumber.version>

		<json-path.version>2.8.0</json-path.version>
		<json-path.gov4j.version>2.8.0-gov4j-1</json-path.gov4j.version>
		<json-smart.version>2.5.0</json-smart.version>
		<accessors-smart.version>2.5.0</accessors-smart.version>

		<jettison.version>1.5.4</jettison.version>

	</properties>

	<dependencies>

		<dependency>
			<groupId>org.json</groupId>
			<artifactId>json</artifactId>
			<version>${org.json.json.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>com.networknt</groupId>
			<artifactId>json-schema-validator</artifactId>
			<version>${networknt_json-schema-validator.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.ethlo.time</groupId>
			<artifactId>itu</artifactId>
			<version>${com-ethlo-time.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>com.github.java-json-tools</groupId>
			<artifactId>json-schema-validator</artifactId>
			<version>${github-java-json-tools_json-schema-validator.gov4j.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.java-json-tools</groupId>
			<artifactId>json-schema-core</artifactId>
			<version>${github-java-json-tools_json-schema-core.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.java-json-tools</groupId>
			<artifactId>jackson-coreutils</artifactId>
			<version>${github-java-json-tools_jackson-coreutils.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.java-json-tools</groupId>
			<artifactId>jackson-coreutils-equivalence</artifactId>
			<version>${github-java-json-tools_jackson-coreutils-equivalence.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.java-json-tools</groupId>
			<artifactId>uri-template</artifactId>
			<version>${github-java-json-tools_uri-template.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.fge</groupId>
			<artifactId>btf</artifactId>
			<version>${btf.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.github.fge</groupId>
			<artifactId>msg-simple</artifactId>
			<version>${msg-simple.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.google.guava</groupId>
			<artifactId>failureaccess</artifactId>
			<version>${failureaccess.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.googlecode.libphonenumber</groupId>
			<artifactId>libphonenumber</artifactId>
			<version>${libphonenumber.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>com.jayway.jsonpath</groupId>
			<artifactId>json-path</artifactId>
			<version>${json-path.gov4j.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>net.minidev</groupId>
			<artifactId>json-smart</artifactId>
			<version>${json-smart.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>net.minidev</groupId>
			<artifactId>accessors-smart</artifactId>
			<version>${accessors-smart.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>org.codehaus.jettison</groupId>
			<artifactId>jettison</artifactId>
			<version>${jettison.version}</version>
			<exclusions>
				<exclusion>
					<groupId>*</groupId>
					<artifactId>*</artifactId>
				</exclusion>
			</exclusions>
		</dependency>

	</dependencies>

	<build>
		<plugins>
			<plugin>
				<!-- per evitare 429: richiesta verso ossindex -->
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>${maven-antrun-plugin}</version>
				<configuration>
					<target>
						<sleep seconds="${owasp.ossindex.prevents429.sleep}" />
					</target>
				</configuration>
				<executions>
					<execution>
						<id>sleep-for-a-while</id>
						<phase>${owasp}</phase>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-clean-plugin</artifactId>
				<version>${maven-clean-plugin}</version>
				<executions>
					<execution>
						<id>clean</id>
						<phase>initialize</phase>
						<goals>
							<goal>clean</goal>
						</goals>
						<configuration>
							<excludeDefaultDirectories>true</excludeDefaultDirectories>
							<filesets>
								<filesets>
									<directory>../../../lib/${jar.group.name}</directory>
									<includes>
										<include>*.jar</include>
									</includes>
								</filesets>
							</filesets>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>${maven-dependency-plugin}</version>
				<executions>
					<execution>
						<id>copy-dependencies</id>
						<phase>initialize</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>../../../lib/${jar.group.name}</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>false</overWriteSnapshots>
							<overWriteIfNewer>true</overWriteIfNewer>
							<failOnMissingClassifierArtifact>true</failOnMissingClassifierArtifact>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<!-- Rename per differenziale i jar 'json-schema-validator-xxx.jar' che hanno stesso artifactId -->
			<plugin>
				<groupId>com.coderplus.maven.plugins</groupId>
				<artifactId>copy-rename-maven-plugin</artifactId>
				<version>1.0</version>
				<executions>
					<execution>
						<id>rename-file-networknt</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/json-schema-validator-${networknt_json-schema-validator.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/networknt_json-schema-validator-${networknt_json-schema-validator.version}.jar</destinationFile>
						</configuration>
					</execution>
					<execution>
						<id>rename-file-github-validator</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/json-schema-validator-${github-java-json-tools_json-schema-validator.gov4j.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/github_json-schema-validator-${github-java-json-tools_json-schema-validator.gov4j.version}.jar</destinationFile>
						</configuration>
					</execution>
					<execution>
						<id>rename-file-github-core</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/json-schema-core-${github-java-json-tools_json-schema-core.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/github_json-schema-core-${github-java-json-tools_json-schema-core.version}.jar</destinationFile>
						</configuration>
					</execution>
					<execution>
						<id>rename-file-github-jackson</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/jackson-coreutils-${github-java-json-tools_jackson-coreutils.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/github_jackson-coreutils-${github-java-json-tools_jackson-coreutils.version}.jar</destinationFile>
						</configuration>
					</execution>
					<execution>
						<id>rename-file-github-jackson-equivalence</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/jackson-coreutils-equivalence-${github-java-json-tools_jackson-coreutils-equivalence.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/github_jackson-coreutils-equivalence-${github-java-json-tools_jackson-coreutils-equivalence.version}.jar</destinationFile>
						</configuration>
					</execution>
					<execution>
						<id>rename-file-github-uri-template</id>
						<phase>initialize</phase>
						<goals>
							<goal>rename</goal>
						</goals>
						<configuration>
							<sourceFile>../../../lib/${jar.group.name}/uri-template-${github-java-json-tools_uri-template.version}.jar</sourceFile>
							<destinationFile>../../../lib/${jar.group.name}/github_uri-template-${github-java-json-tools_uri-template.version}.jar</destinationFile>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
